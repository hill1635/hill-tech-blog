<div class="body">
{{#each myPosts as |myPost| }}
    {{!-- Cycle through all Post posts --}}
    <div class="myPost">
        <div class="header">
            <a href="">{{myPost.title}}</a>
            <p>Posted by {{myPost.username}} on {{myPost.datePosted}}</p>
        </div>
        <div class="content">
            <p>{{myPost.content}}</p>
        </div>
    </div>
{{/each}}
</div>
    <button class="addPost">Add Post</button>

    <script src="js/post.js"></script>