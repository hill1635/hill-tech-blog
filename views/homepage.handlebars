{{#each posts as |post| }}
<div class="body">
    {{!-- Cycle through all post posts --}}
    <div class="post">
        <div class="header">
            <a href="">{{post.title}}</a>
            <p>Posted by {{user.username}} on {{post.createdAt}}</p>
        </div>
        <div class="content">
            <p>{{post.content}}</p>
            <button>Comment</button>
        </div>
    </div>
</div>
{{/each}}